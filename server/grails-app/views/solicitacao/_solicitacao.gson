import br.com.hospitaldocoracaoal.hsup.Solicitacao

model {
    Solicitacao solicitacao
}

json g.render(solicitacao, [excludes: ['responsavel', 'itens.solicitacao'], expand: ['setor', 'itens', 'produto','status'], resolveTemplate: false]) {
    responsavel {
        id solicitacao.responsavel.id
        name solicitacao.responsavel.name
        setor solicitacao.responsavel.setor.nome
    }
}
